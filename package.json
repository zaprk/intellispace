{
  "name": "intellispace-mvp",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:vite\" \"wait-on http://localhost:3000 && npm run dev:electron\"",
    "dev:vite": "vite",
    "dev:backend": "tsx watch src/backend/server.ts",
    "dev:electron": "cross-env NODE_ENV=development electron .",
    "build": "npm run build:backend && npm run build:renderer && npm run build:main",
    "build:renderer": "vite build",
    "build:main": "tsc -p tsconfig.main.json",
    "build:backend": "tsc -p tsconfig.backend.json",
    "dist": "npm run build && electron-builder",
    "postinstall": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/node": "^24.2.1",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@types/styled-components": "^5.1.34",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^5.0.0",
    "concurrently": "^9.2.0",
    "cross-env": "^10.0.0",
    "electron": "^37.2.6",
    "electron-builder": "^26.0.12",
    "nodemon": "^3.1.10",
    "tsx": "^4.20.3",
    "typescript": "^5.9.2",
    "vite": "^7.1.1",
    "wait-on": "^8.0.4"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.59.0",
    "@langchain/core": "^0.3.69",
    "@langchain/langgraph": "^0.4.4",
    "@langchain/ollama": "^0.2.3",
    "@prisma/client": "^6.13.0",
    "axios": "^1.11.0",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "immer": "^10.1.1",
    "lucide-react": "^0.539.0",
    "ollama": "^0.5.17",
    "openai": "^5.12.2",
    "prisma": "^6.13.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "sqlite3": "^5.1.7",
    "styled-components": "^6.1.19",
    "uuid": "^11.1.0",
    "zustand": "^5.0.7"
  }
}
